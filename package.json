{
  "name": "papikondalu-tourism",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "seo:check": "next build && next start",
    "seo:audit": "lighthouse http://localhost:3000 --only-categories=seo --output html --output-path ./seo-audit.html",
    "perf:audit": "lighthouse http://localhost:3000 --only-categories=performance --output html --output-path ./performance-audit.html",
    "optimize": "next build && next-bundle-analyzer"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@types/nodemailer": "^7.0.1",
    "critters": "^0.0.23",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "next": "14.2.0",
    "nodemailer": "^7.0.5",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^6.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
